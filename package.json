{
  "name": "cal-rule",
  "version": "0.4.0",
  "description": "A simple javascript/typescript calculating rule solution. Support `1A&2B|!(3C&4D)`",
  "main": "dist/index.js",
  "scripts": {
    "c": "npm run commit",
    "commit": "git add . && git status && git-cz",
    "prepare": "husky install",
    "lint": "eslint src/* --fix",
    "test": "jest",
    "test:cov": "jest --coverage",
    "build": "tsc",
    "dev": "tsc --watch",
    "ci": "npm run lint && npm run build",
    "release": "npm run build && gulp"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darkXmo/cal-rule.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/darkXmo/cal-rule/issues"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --cache --fix",
      "prettier --write  --ignore-unknown"
    ],
    "*.{ts,js,md,cjs,json}": "prettier --write  --ignore-unknown"
  },
  "homepage": "https://github.com/darkXmo/cal-rule#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@jest/types": "^28.1.3",
    "@types/jest": "^28.1.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "chalk": "^5.0.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^10.1.0",
    "git-cz": "^4.9.0",
    "gulp": "^4.0.2",
    "husky": "^8.0.1",
    "inquirer": "^9.0.1",
    "jest": "^28.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "typescript": "^4.7.4"
  }
}
